#include <stdio.h>

int main () {
    int row, col;
    for (row = 1; row <= 7; row++) {
        for (col = 1; col <= 7; col++) {
            if (col == 1 || (row == 4 && (col == 3)) || (row == 3 && (col == 4)) || (row == 5 && (col == 4)) || (row == 2 && (col == 5)) || (row == 6 && (col == 5)) || (row == 1 && (col == 6)) || (row == 7 && (col == 6))) {
                printf("* ");
            } else printf("  ");
        }
        printf("\n");
    }
    printf("\n");
    for (row = 1; row <= 7; row++) {
        printf("\t\t\t");
        for (col = 1; col <= 7; col++) {
            if ((col == 1 && (row != 1)) || (col == 2 && (row == 1 || row == 4)) || (col == 3 && (row == 1 || row == 4)) || (col == 4 && (row == 1 || row == 4)) || (col == 5 && (row == 1 || row == 4)) || (col == 6 && (row == 1 || row == 4)) || (col == 7 && (row != 1))) {
                printf("* ");
            } else printf("  ");
        }
        printf("\n");
    }
    printf("\n");
    for (row = 1; row <= 7; row++) {
        printf("\t\t\t\t\t\t");
        for (col = 1; col <= 7; col++) {
            if ((col == 1 && (row == 1)) || (col == 2 && (row == 2)) || (col == 3 && (row == 3)) || (col == 4 && (row != 1 && row != 2 && row != 3)) || (col == 5 && (row == 3)) || (col == 6 && (row == 2)) || (col == 7 && (row == 1))) {
                printf("* ");
            } else printf("  ");
        }
        printf("\n");
    }
    printf("\n");
    for (row = 1; row <= 7; row++) {
        printf("\t\t\t\t\t\t\t\t\t");
        for (col = 1; col <= 7; col++) {
            if (row == 1 || col == 1 || row == 4 || row == 7) {
                printf("* ");
        } else printf(" ");
    }
    printf("\n");
}
return 0;
}
