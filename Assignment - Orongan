#include <stdio.h>

int main() {
    int row, col, space = 1;
    
    for(row=1; row<=5; row++) {
        for(col=1;col<=5;col++) {
        if(row==1 || (col==1 &&(row==2 || row==3)) || row==3 || (row==4 &&(col==5)) || row==5)
        printf("* ");
        else printf("  ");
        }
     printf("\n");
    }
    printf("\n");
    
    for(row=1;row<=5;row++) {
        for(space=1;space<=3;space++)
        printf(" ");
    for(col=1;col<=5;col++) {
    if(row==1 || col==1 || row==3 || row==5)
    printf("* ");
    else printf("  ");
    }
    printf("\n");
    }
        printf("\n");
    
    for(row=1;row<=5;row++) {
        for(space=1;space<=9;space++)
        printf(" ");
    for(col=1;col<=5;col++) {
    if(row==1 || col==1 || (row==2 &&(col==5)) || row==3)
    printf("* ");
    else printf("  ");
    }
    printf("\n");
    }
       printf("\n");
    
    for(row=1;row<=5;row++) {
        for(space=1;space<=12;space++)
        printf(" ");
    for(col=1;col<=5;col++) {
    if(col==1 || row==3 || col==5)
    printf("* ");
    else printf("  ");
    }
    printf("\n");
    }
}
