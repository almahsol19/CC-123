#include <stdio.h>

int main() {
    int row, col, space = 1;

    // D 
    for(row=1;row<=5;row++) {
        for(space=1;space<=0;space++)
        printf("  ") ;
        for(col=1;col<=5;col++) {
            if(col==1 || (row==1 && col!=5) || (row==5 && col!=5) || (col==5 && row!=1 && row!=5))
            printf("* ");
            else printf("  ");
        }
        printf("\n");
    }
    printf("\n");

    // A
    for(row=1;row<=5;row++) {
        for(space=1;space<=8;space++)
        printf(" ");
        for(col=1;col<=5;col++) {
            if(col==1 || col==5 || (row==1 && col!=1) || (row==3 && col!=1 && col!=5))
            printf("* ");
            else printf("  ");
        }
        printf("\n");
    }
    printf("\n");

    // L
    for(row=1;row<=5;row++) {
        for(space=1;space<=16;space++)
        printf(" ");
        for(col=1;col<=5;col++) {
            if(col==1 || row==5)
            printf("* ");
            else printf("  ");
        }
        printf("\n");
    }
    printf("\n");

    // I
    for(row=1;row<=5;row++) {
        for(space=1;space<=24;space++)
        printf(" ");
        for(col=1;col<=5;col++) {
            if(col==3 || row==1 || row==5)
            printf("* ");
            else printf("  ");
        }
        printf("\n");
    }
    printf("\n");

    // G
    for(row=1;row<=5;row++) {
        for(space=1;space<34;space++)
        printf(" ");
        for(col=1;col<=5;col++) {
            if(col==1 || row==1 || row==5 || (col==5 && row>=3) || (row==3 && col >= 3))
            printf("* ");
            else printf("  ");
        }
        printf("\n");
    }
    printf("\n");
}
    
