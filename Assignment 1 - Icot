#include <stdio.h>

int main() {
    int i, j, space;

    for (i = 0; i < 5; i++) {
        for (space = 0; space < 0; space++) {
            printf(" ");
        }
        for (j = 0; j < 5; j++) {
            if (i == 0 || i == 4 || j == 2) {
                printf("*");
            } else {
                printf(" ");
            }
        }
        printf("\n");
    }
    printf("\n");
    
    for (i = 0; i < 5; i++) {
        for (space = 0; space < 5; space++) {  
            printf(" ");
        }
        for (j = 0; j < 5; j++) {
            if (i == 0 || i == 4 || j == 0) {
                printf("*");
            } else {
                printf(" ");
            }
        }
        printf("\n");
    }
    printf("\n");
    
    for (i = 0; i < 5; i++) {
        for (space = 0; space < 10; space++) {  
            printf(" ");
        }
        for (j = 0; j < 5; j++) {
            if (i == 0 || i == 4 || j == 0 || j == 4) {
                printf("*");
            } else {
                printf(" ");
            }
        }
        printf("\n");
    }
    printf("\n");
    
    for (i = 0; i < 5; i++) {
        for (space = 0; space < 15; space++) {  
            printf(" ");
        }
        for (j = 0; j < 5; j++) {
            if (i == 0 || j == 2) {
                printf("*");
            } else {
                printf(" ");
            }
        }
        printf("\n");
    }
    printf("\n");

    return 0;
}
    
