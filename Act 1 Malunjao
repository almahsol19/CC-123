#include <stdio.h>
#include <stdlib.h>
#include <time.h>

int main() {
    int number, guess, tries = 0;
   
    srand(time(NULL));
    
    number = rand() % 100 + 1;

    printf("Hey what's up,try to guess a number\n");
    printf("only 1 to 100\n");

    do {
        printf("Take a guess: ");
        scanf("%d", &guess);
        tries++;

        if (guess < number) {
            printf("Too low!\n");
        } else if (guess > number) {
            printf("Too high!\n");
        } else {
            printf("Congratulations! naka %d tries. ka HAHAHA\n", tries);
        }
    } while (guess != number);

    return 0;
}
