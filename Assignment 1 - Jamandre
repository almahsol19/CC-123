#include <stdio.h>

int main() {
    int row, column;


     // Letter J
    for (row = 1; row <= 5; row++) {
        for (column = 1; column <= 5; column++) {
            if (column == 5 || row == 5) {
                printf("* ");
            }else printf("  ");
        }
        printf("\n"); 
    }


    // Letter A
    for (row = 1; row <= 5; row++) {
        for (int space = 0; space < 5; space++) { // No extra space for first letter
            printf("  ");
        }
        for (column = 1; column <= 5; column++) {
            if ((row == 1 && column != 1 && column != 5) || 
                (row == 2 && (column == 1 || column == 5)) || 
                (row == 3) || 
                (row == 4 && (column == 1 || column == 5)) || 
                (row == 5 && (column == 1 || column == 5))) {
                printf("* ");
            } else {
                printf("  ");
            }
        }
        printf("\n");
    }
    printf("\n");

    // Letter M
    for (row = 1; row <= 5; row++) {
        for (int space = 0; space < 10; space++) { // Shift right for slanted effect
            printf("  ");
        }
        for (column = 1; column <= 5; column++) {
            if (column == 1 || column == 5 || (row == 2 && column == 2) || (row == 3 && column == 3) || (row == 2 && column == 4)) {
                printf("* ");
            } else {
                printf("  ");
            }
        }
        printf("\n");
    }
    printf("\n");

    // Letter A
    for (row = 1; row <= 5; row++) {
        for (int space = 0; space < 15; space++) { // Shift right
            printf("  ");
        }
        for (column = 1; column <= 5; column++) {
            if ((row == 1 && column != 1 && column != 5) || 
                (row == 2 && (column == 1 || column == 5)) || 
                (row == 3) || 
                (row == 4 && (column == 1 || column == 5)) || 
                (row == 5 && (column == 1 || column == 5))) {
                printf("* ");
            } else {
                printf("  ");
            }
        }
        printf("\n");
    }
    printf("\n");

    return 0;
    
    }
    
