#include <stdio.h>

int main() {
    int i, j;

    // First pattern (Letter V )
    for (i = 0; i < 5; i++) {
        
        for (j = 0; j < i; j++) {
            printf(" ");
        }
        
        printf("*");


        for (j = 0; j < 8 - 2 * i; j++) {
            printf(" ");
        }

        
        if (i != 4) {
            printf("*");
        }
        printf("\n");
    }

    
    printf("\n");

    // Second pattern (Letter I)
    for (i = 0; i < 5; i++) {
        
        for (j = 0; j < 12; j++) {
            printf(" ");
        }
        
        if (i == 0 || i == 4) {
            printf("*****");
        } else {
            printf("  *");
        }
        printf("\n");
    }

    
    printf("\n");

    // Third pattern (Letter C)
    for (i = 0; i < 5; i++) {
        
        for (j = 0; j < 16; j++) {
            printf(" ");
        }
        
        if (i == 0 || i == 4) {
            printf("*****");
        } else {
            printf("*");
        }
        printf("\n");
    }

    
    printf("\n");

    // Fourth pattern (Letter E)
    for (i = 0; i < 5; i++) {
        // Print leading spaces
        for (j = 0; j < 20; j++) {
            printf(" ");
        }

        if (i == 0 || i == 2 || i == 4) {
            printf("*****");
        } else {
            printf("*");
        }
        printf("\n");
    }

    return 0;
}
