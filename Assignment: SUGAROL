#include <stdio.h>

int main() {
int row, col, space = 1;

    for (row = 0; row < 6; row++) {
        for (col = 0; col < 5; col++) {
            if (row == 0 || row == 2 || row == 5 || (row > 0 && row <2&& col ==  0) || (row > 2&& row < 5 && col == 4)) {
                printf("*");
            } else {
                printf(" ");
            }
        }
        printf("\n");
    }
    
    for (row = 0; row < 5; row++) {
        printf("\t" );
        for (col = 0; col < 5; col++) {
            if (col == 0 || col == 4 || (row == 4 && col > 0 && col < 4)) {
                printf("*");
            } else {
                printf(" ");
            }
        }
        printf("\n");
    }
    
  for(row=1;row<=5;row++) {
        printf("\t\t");
        for(col=1;col<=5;col++) {
            if(col== 1 | row== 1 || (row==3 &&(col== 3 || col==4 || col==4)) || (row==4&&(col==4)) || row==5 )
            printf("*");
            else printf("  ");
        }
        printf("\n");
  }
  
    for (row = 0; row <= 5; row++) {
        printf("\t\t\t*");
        for (col = 1; col <= 5; col++) {
            if (row == 0 || row == 2|| col== 5|| col == 5) {
                printf("*");
            } else {
                printf(" ");
            }
        }
        printf("\n");
    }
  return 0;
  }
