#include <stdio.h>

int main() {
    
    for (int i = 0; i < 1; i++) {
        printf("******\n");
        for (int j = 0; j < 1; j++) {
            printf("*    *\n");
        }
        printf("******\n");
    }

    
    printf("*\n");
    for (int i = 0; i < 1; i++) {
        printf("*          **\n");
        for (int j = 0; j < 1; j++) {
            printf("          *   *\n");
        }
        for (int k = 0; k < 1; k++) {
            printf("          *****\n");
        }
        printf("          *   *\n");
        printf("          *   *\n");
    }

    
    for (int i = 0; i < 4; i++) {
        printf("                    *\n");
    }
    printf("                    ******\n");

    
    for (int i = 0; i < 1; i++) {
        printf("                               ******\n");
        for (int j = 0; j < 1; j++) {
            printf("                             *        *\n");
        }
        for (int k = 0; k < 1; k++) {
            printf("                             *        *\n");
        }
        for (int l = 0; l < 1; l++) {
            printf("                             *        *\n");
        }
        printf("                               ******\n");
    }

    return 0;
}
