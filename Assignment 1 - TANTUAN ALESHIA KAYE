#include <stdio.h>

int main() {
    int row, column;

    // Print letter T no need spacing
    for(row = 1; row <= 5; row++) {
        for(column = 1; column <= 5; column++) {
            if (row == 1 || column == 3) // Top row and middle column
                printf("* ");
            else
                printf("  ");
        }
        printf("\n");
    }
    printf("\n");

    // Print letter A start nag spacing diri for letters para ma slant siya 
    for(row = 1; row <= 5; row++) {
        for(int space = 0; space < 2; space++) {
            printf("  "); // Spaces to shift each letter
        }
        for(column = 1; column <= 5; column++) { //ari na Mag start ug form sa letters by the use of asterisk
            if (row == 1 && column != 1 && column != 5 || // column of each asterisk para ma form ug letters
                (row == 2 && (column == 1 || column == 5)) || 
                (row == 3) || 
                (row == 4 && (column == 1 || column == 5)) || 
                (row == 5 && (column == 1 || column == 5)))
                printf("* "); // kani siya is para dili ma bungkag imong gi form na asterisk
            else
                printf("  "); // if imo nang e try ug space ma hiwi imohang letter asterisk
        }
        printf("\n");
    }
    printf("\n");

    // Print letter N (shifted right)
    for(row = 1; row <= 5; row++) {
        for(int space = 0; space < 4; space++) {
            printf("  ");
        }
        for(column = 1; column <= 5; column++) {
            if (column == 1 || column == 5 || column == row)
                printf("* ");
            else
                printf("  ");
        }
        printf("\n");
    }
    printf("\n");

    // Print letter T (shifted right)
    for(row = 1; row <= 5; row++) {
        for(int space = 0; space < 6; space++) {
            printf("  "); 
        }
        for(column = 1; column <= 5; column++) {
            if (row == 1 || column == 3) 
                printf("* ");
            else
                printf("  ");
        }
        printf("\n");
    }
    printf("\n");

    // Print letter U (shifted right)
    for(row = 1; row <= 5; row++) {
        for(int space = 0; space < 8; space++) {
            printf("  "); 
        }
        for(column = 1; column <= 5; column++) {
            if ((column == 1 && row != 5) || (column == 5 && row != 5) || (row == 5 && column != 1 && column != 5))
                printf("* ");
            else 
                printf("  ");
        }
        printf("\n");
    }
    printf("\n");

    // Print letter A (shifted right)
    for(row = 1; row <= 5; row++) {
        for(int space = 0; space < 10; space++) {
            printf("  "); 
        }
        for(column = 1; column <= 5; column++) {
            if (row == 1 && column != 1 && column != 5 || 
                (row == 2 && (column == 1 || column == 5)) || 
                (row == 3) || 
                (row == 4 && (column == 1 || column == 5)) || 
                (row == 5 && (column == 1 || column == 5)))
                printf("* ");
            else
                printf("  ");
        }
        printf("\n");
    }
    printf("\n");

    // Print letter N (shifted right)
    for(row = 1; row <= 5; row++) {
        for(int space = 0; space < 12; space++) {
            printf("  "); 
        }
        for(column = 1; column <= 5; column++) {
            if (column == 1 || column == 5 || column == row)
                printf("* ");
            else
                printf("  ");
        }
        printf("\n");
    }

    return 0;
}
