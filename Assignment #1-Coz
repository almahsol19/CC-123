#include <stdio.h>

int main() {
    int row, col, space = 1;
    
    for(row=1;row<=5;row++) {
        for(col=1;col<=5;col++) {
            if(row==1 || col==1 || row==5)
            printf("* ");
            else printf("  ");
        }
        printf("\n");
    }
    printf("\n");
    
    for(row=1;row<=5;row++) {
        for(space=1;space<=10;space++)
          printf(" ");
          for(col=1;col<=5;col++) {
              if(row==1 || col==1 || row==5 || col==5)
              printf("* ");
              else printf("  ");
          }
          printf("\n");
    }
    printf("\n");
    
    for(row=1;row<=5;row++) {
        for(space=1;space<=20;space++)
          printf(" ");
          for(col=1;col<=5;col++) {
              if(row==1 || (row==2 &&(col==4) || (row==3 &&(col==3) || (row==4 &&(col==2) || row==5))))
              printf("* ");
              else printf("  ");
          }
          printf("\n");
    }
    printf("\n");
}
