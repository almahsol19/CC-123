#include <iostream>
using namespace std;

int main() {
    int arr[] = {1, 6, 7, 9, 9, 4, 5, 6, 7, 8, 9, 6, 4, 3, 3};
    int numToFind;

    cout << "Enter the number to find its frequency: ";
    cin >> numToFind;

    int frequency = 0;

    for (int i = 0; i < sizeof(arr) / sizeof(arr[0]); i++) {
        if (arr[i] == numToFind) {
            frequency++;
        }
    }

    cout << "The frequency" " is " << frequency << endl;

    return 0;
}
