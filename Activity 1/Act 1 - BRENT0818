#include <stdio.h>  // Include the standard input-output library

int main() {  // Main function where program execution begins

    int num;  // Declare an integer variable to store user input

    // Ask the user to enter a number
    printf("Enter a number: ");
    scanf("%d", &num);  // Read the user input and store it in 'num'

    // Check if the input number is greater than or equal to 10
    if (num >= 10) {
        printf("HIGH");  // Print "HIGH" if the condition is true
    } else {
        printf("LOW");   // Print "LOW" if the condition is false
    }

    return 0;  // Return 0 to indicate successful execution
}
