 #include <stdio.h>

int main() {
    int number;

    for (int i = 0; i < 5; i++) { //so akong gi gamit is for loop para igka 5 ra mag input ang user 
        printf("Enter a number: ");
        scanf("%d", &number);

        if (number >= 1 && number <= 59) { //nag gamit kog if else para ma hibaw an nako if to high bana siya or to low 
            printf("Too low!\n");
        } else if (number >= 60 && number <= 100) {
            printf("Too high!\n");
        } else {
            printf("Invalid input or neutral range.\n"); // ako ni gi include para ma hibaw an nga kana nga number nga imo gi imput is valid ba na or invalid basta di lang ma lapas sa number nga akong gi indicate 
        }
    }
    
    printf("Sucess\n"); // ako ni gi indicate kay ni run akong code

    return 0;
