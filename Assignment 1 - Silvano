#include<stdio.h>

int main()
{
    int firstloop, secondloop, space, height = 7;
    
    //print J
    for(firstloop = 0; firstloop < height; firstloop++){
        for(secondloop = 0; secondloop < height; secondloop++){
            if(secondloop != 6 && firstloop != 6){
                printf(" ");
            }else if(firstloop == 6){
                printf("*******");
                break;
            }else printf("*");
        }
        printf("\n");
    }
    
    //print U
    for(firstloop = 0; firstloop < height; firstloop++){
        printf("\t");
        for(secondloop = 0; secondloop < height; secondloop++){
            if((secondloop == 0 && (firstloop != 6)) || (secondloop == 6 && (firstloop != 6)) || (firstloop == 6 && (secondloop != 0 && secondloop != 6))){
                printf("*");
            }else printf(" ");
        }
        printf("\n");
    }
    
    //print A
    for (firstloop = 0; firstloop < height; firstloop++) {
        printf("\t\t");
        for (secondloop = 0; secondloop < height; secondloop++) {
            if ((firstloop == 0 && secondloop > 0 && secondloop < height - 1) || (secondloop == 0 && firstloop > 0) || (secondloop == height - 1 && firstloop > 0) || (firstloop == height / 2 && secondloop > 0 && secondloop < height - 1)) {
                printf("*");
            } else {
                printf(" ");
            }
        }
        printf("\n");
    }
    
    //print N
    for (firstloop = 0; firstloop < height; firstloop++) {
        printf("\t\t\t");
        for (secondloop = 0; secondloop < height - 1; secondloop++) {
            if (secondloop == 0 || secondloop == firstloop) {
                printf("*");
            } else {
                printf(" ");
            }
        }
        printf("*\n");
    }
    
    return 0;
}    
