#include <stdio.h>

int main() {
    int row, col, space = 1;
    for(row=1;row<=5;row++) { //kani siya loop sir kay for letter M nya kaning for loop nga row ug col sir kay maka display nana siyag box nga pattern
        for(col=1;col<=5;col++) {
            if(col==1 || (row==2 &&(col==2|| col==4)) || (row==3 &&(col==3)) || col==5) // nya diri ko nag modify para maka form kog letter M gamit ra ang if else statement
                printf("* ");
                else printf("  ");
        }
        printf("\n");
    }
    printf("\n"); // ga new line ko ari sir para ma butang nakos ubos ang another letter 

    for(row=1;row<=5;row++) {
        for(space=1;space<=3;space++) // nya ari sir kay nag add kog for loop para sa spacing para maka display ko sa position nga pina slanting nya gi by 3 ra nako space<=3
        printf(" ");
        for(col=1;col<=5;col++) {
           if(col==1 || col==5 || row==1 || row==3)
            printf("* ");
            else printf("  ");
        }
        printf("\n");
    }
    printf("\n");

    for(row=1;row<=5;row++) {
        for(space=1;space<=6;space++)
        printf(" ");
        for(col=1;col<=5;col++) {
            if(col==1 || row==5)
            printf("* ");
            else printf("  ");
        }
        printf("\n");
    }
    printf("\n");

    for(row=1;row<=5;row++) {
        for(space=1;space<=9;space++)
        printf(" ");
        for(col=1;col<=5;col++) {
            if(col==1 || row==5 || col==5)
            printf("* ");
            else printf("  ");
        }
        printf("\n");
    }
    printf("\n");

    for(row=1;row<=5;row++) {
        for(space=1;space<=12;space++)
        printf(" ");
        for(col=1;col<=5;col++) {
            if(col==1 || row==2 &&(col==2) || row==3 &&(col==3) || row==4 &&(col==4) || col==5)
            printf("* ");
            else printf("  ");
        }
        printf("\n");
    }
    printf("\n");

    for(row=1;row<=5;row++) {
        for(space=1;space<=15;space++)
        printf(" ");
        for(col=1;col<=5;col++) {
            if(col==1 &&(row==4 || row==5) || row==5 || col==5)
            printf("* ");
            else printf("  ");
        }
        printf("\n");
    }
    printf("\n");

    for(row=1;row<=5;row++) {
        for(space=1;space<=18;space++)
        printf(" ");
        for(col=1;col<=5;col++) {
            if(col==1 || col==5 || row==1 || row==3)
            printf("* ");
            else printf("  ");
        }
        printf("\n");
    }
    printf("\n");

    for(row=1;row<=5;row++) {
        for(space=1;space<=21;space++)
        printf(" ");
        for(col=1;col<=5;col++) {
            if(col==1 || row==1 || row==5 || col==5)
            printf("* ");
            else printf("  ");
        }
        printf("\n");
    }
}
