#include <stdio.h>

void print_D(int offset) {
    for (int i = 0; i < 7; i++) {
        for (int s = 0; s < offset; s++) printf(" ");
        for (int j = 0; j < 7; j++) {
            if ((j == 0) || (i == 0 && j != 6) || (i == 6 && j != 6) || (j == 6 && i != 0 && i != 6)) {
                printf("*");
            } else {
                printf(" ");
            }
        }
        printf("\n");
    }
}

void print_E(int offset) {
    for (int i = 0; i < 7; i++) {
        for (int s = 0; s < offset; s++) printf(" ");
        for (int j = 0; j < 7; j++) {
            if ((j == 0) || (i == 0) || (i == 3) || (i == 6)) {
                printf("*");
            } else {
                printf(" ");
            }
        }
        printf("\n");
    }
}

void print_S(int offset) {
    for (int i = 0; i < 7; i++) {
        for (int s = 0; s < offset; s++) printf(" ");
        for (int j = 0; j < 7; j++) {
            if ((i == 0 && j != 0) ||  
                (i == 3) ||            
                (i == 6 && j != 6) ||  
                (j == 0 && i > 0 && i < 3) ||  
                (j == 6 && i > 3 && i < 6)) { 
                printf("*");
            } else {
                printf(" ");
            }
        }
        printf("\n");
    }
}

void print_I(int offset) {
    for (int i = 0; i < 7; i++) {
        for (int s = 0; s < offset; s++) printf(" ");
        for (int j = 0; j < 7; j++) {
            if (i == 0 || i == 6 || j == 3) { 
                printf("*");
            } else {
                printf(" ");
            }
        }
        printf("\n");
    }
}

void print_R(int offset) {
    for (int i = 0; i < 7; i++) {
        for (int s = 0; s < offset; s++) printf(" ");
        for (int j = 0; j < 7; j++) {
            if (j == 0 || (i == 0 && j != 6) || (i == 3 && j != 6) || (j == 6 && i > 0 && i < 3) || (i == j && i > 3)) {
                printf("*");
            } else {
                printf(" ");
            }
        }
        printf("\n");
    }
}

void print_T(int offset) {
    for (int i = 0; i < 7; i++) {
        for (int s = 0; s < offset; s++) printf(" ");
        for (int j = 0; j < 7; j++) {
            if (i == 0 || j == 3) { 
                printf("*");
            } else {
                printf(" ");
            }
        }
        printf("\n");
    }
}

void print_O(int offset) {
    for (int i = 0; i < 7; i++) {
        for (int s = 0; s < offset; s++) printf(" ");
        for (int j = 0; j < 7; j++) {
            if ((i == 0 && j != 0 && j != 6) || (i == 6 && j != 0 && j != 6) || 
                (j == 0 && i != 0 && i != 6) || (j == 6 && i != 0 && i != 6)) {
                printf("*");
            } else {
                printf(" ");
            }
        }
        printf("\n");
    }
}

int main() {
    int offset = 0;
    
    print_D(offset);
    offset += 10;
    printf("\n");
    
    print_E(offset);
    offset += 10;
    printf("\n");
    
    print_S(offset);
    offset += 10;
    printf("\n");
    
    print_I(offset);
    offset += 10;
    printf("\n");
    
    print_E(offset);
    offset += 10;
    printf("\n");
    
    print_R(offset);
    offset += 10;
    printf("\n");
    
    print_T(offset);
    offset += 10;
    printf("\n");
    
    print_O(offset);
    
    return 0;
}
