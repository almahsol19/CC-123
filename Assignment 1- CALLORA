#include <stdio.h>

int main() {
    int rows, columns, space;
    
    // Printing C
    for (rows = 0; rows < 5; rows++) {
        for (columns = 0; columns < 5; columns++) {
            if (rows == 0 || rows == 4 || columns == 0) {
                printf("*");
            } else {
                printf(" ");
            }
        }
        printf("\n");
    }
    printf("\n");
    
    //Printing A
    for (rows = 0; rows < 5; rows++) {
        for (space = 0; space < 5; space++) {  
            printf(" ");
        }
        for (columns = 0; columns < 5; columns++) {
            if (rows == 0 || columns == 0 || rows == 2 || columns == 4) {
                printf("*");
            } else {
                printf(" ");
            }
        }
        printf("\n");
    }
    printf("\n");
    
    // Printing L
    for (rows = 0; rows < 5; rows++) {
        for (space = 0; space < 10; space++) {  
            printf(" ");
        }
        for (columns = 0; columns < 5; columns++) {
            if ( rows == 4 || columns == 0 ) {
                printf("*");
            } else {
                printf(" ");
            }
        }
        printf("\n");
    }
    printf("\n");
    
    // Printing L
    for (rows = 0; rows < 5; rows++) {
        for (space = 0; space < 15; space++) {  
            printf(" ");
        }
        for (columns = 0; columns < 5; columns++) {
            if (rows == 4 || columns == 0) {
                printf("*");
            } else {
                printf(" ");
            }
        }
        printf("\n");
    }
    printf("\n");

    return 0;
}
